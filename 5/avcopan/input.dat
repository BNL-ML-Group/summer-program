# doublet H2O+

molecule mol {
  1 2
  O
  H 1 0.96
  H 1 0.96 2 104.5
}

set {
  basis sto-3g
  e_convergence 12
  scf_type pk
  maxiter 50
  reference uhf
}

basisname = psi4.get_global_option("BASIS")
basisset  = psi4.BasisSet.pyconstruct_orbital(mol, "BASIS", basisname)
mints     = psi4.MintsHelper(basisset)

# Call your Hartree-Fock code here
from uhf import UHF

uhf = UHF(mol, mints)
uhf.compute_energy()

# check your answer
print('Compare to:')
print('{:27.15f}'.format(energy('scf')))
