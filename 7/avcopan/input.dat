# doublet H2O+

molecule mol {
  0 1
  O
  H 1 1.1
  H 1 1.1 2 104.0
}

set {
  reference uhf
  basis sto-3g
  e_convergence 10
  scf_type pk
  MAXITER 50
}

# Used to initialize everything
mol.update_geometry()

mints = psi4.MintsHelper()

# Call your Hartree-Fock code here
from cis import CIS

cis = CIS(mol, mints)
cis.compute_excitation_energies()

