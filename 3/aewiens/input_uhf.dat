molecule mol {
   0 1
   F 0.0 0.0 0.0
   F 0.0 0.0 2.0
   }

set {
    basis cc-pvdz
    scf_type pk
    e_convergence 12
	MAXITER 75
    }

Eref = energy('uhf')

# initialize molecule object, integrals library

mol = psi4.get_active_molecule()
mints = psi4.MintsHelper()

# call Hartree-Fock code here
from uhf import UHF
uhf = UHF(mol, mints)
uhf.compute_energy()
print( uhf.natural_orbitals() )
