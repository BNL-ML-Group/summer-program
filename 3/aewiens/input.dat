import psi4

molecule mol {
   O 
   H 1 0.96
   H 1 0.96 2 104.5
   }

set {
    reference rhf
    basis sto-3g
    scf_type pk
    e_convergence 12
	 MAXITER 50
    mp2_type conv
    }

Eref = energy('scf')

basisname = psi4.get_global_option("BASIS")
basisset = psi4.BasisSet.pyconstruct_orbital(mol,"BASIS",basisname)
mints    = psi4.MintsHelper(basisset)

# call Hartree-Fock code here
from rhf import RHF
rhf = RHF(mol,mints)
E = rhf.get_energy()

print(Eref)
print(E)
